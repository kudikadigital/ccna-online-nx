generator client {
  provider = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider = "postgresql"
}

model Lead {
  id        Int      @id @default(autoincrement())
  nome      String
  email     String
  whatsapp  String
  pagamento String
  createdAt DateTime @default(now())
}

model Admin { // Recomendo 'Admin' com 'A' mai√∫sculo (PascalCase)
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}